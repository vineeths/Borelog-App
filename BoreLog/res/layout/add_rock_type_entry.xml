       		    android:layout_width="wrap_content"
       		    android:layout_height="wrap_content"
       		    android:layout_toLeftOf="@+id/fromDepthUnit"
       		    android:layout_centerVertical="true"
       		    android:gravity="right"
       		    android:ems="6"
       		    android:paddingRight="2dp"
